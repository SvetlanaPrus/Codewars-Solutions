//https://www.codewars.com/kata/5d5ee4c35162d9001af7d699/train/javascript


function sumOfMinimums(arr) {
let newArr = [];
let sum = 0;
  for (let i = 0; i < arr.length; i++){
    newArr.push( Math.min(...arr[i]) );
  }
  
  for (let j = 0; j < newArr.length; j++){
    sum = sum + newArr[j];
  }
  return sum;
}
